{
	"name" : "sunhill/visual",
	"description" : "The sunhill visual components",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Klaus Dimde",
			"email" : "klaus@dimde.info"
		}
	],
	"minimum-stability" : "dev",
        "repositories" : [{
                        "type" : "path",
                        "url" : "../basic",
                        "options" : {
                                "symlink" : true
                        }
                },
                {
                        "type" : "path",
                        "url" : "../orm",
                        "options" : {
                                "symlink" : true
                        }
                },
                {
                        "type" : "path",
                        "url" : "../infomarket",
                        "options" : {
                                "symlink" : true
                        }
                }],
         "require" : {
                "laravel/framework" : "^9.2",
                "sunhill/basic": "*",
                "sunhill/orm": "*",
                "sunhill/infomarket": "*"
        },
        "require-dev" : {
                "phpunit/phpunit" : "^9.5.10",
                "orchestra/testbench" : "7.x-dev"
        },
	"autoload" : {
		"psr-4" : {
			"Sunhill\\Visual\\Tests\\" : "tests",
			"Sunhill\\Visual\\Test\\" : "tests",
			"Sunhill\\Visual\\" : "src"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Sunhill\\Visual\\VisualServiceProvider"
			]
		}
	}
}